{
  "expo": {
    "name": "parrots",
    "slug": "parrots",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ParrotsWhiteBg.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/ParrotsWhiteBg.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.zenforest.parrots",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.938579686654-3l1dc47s6i61d0s2qif1cvajh3fnfkvq"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.zenforest.parrots",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCUD-Kmpu_6En3uwKZprZHtCziqNwyl8XU"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "@react-native-google-signin/google-signin",
      "expo-font"
    ]
  }
}
