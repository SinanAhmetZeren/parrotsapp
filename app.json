{
  "expo": {
    "name": "parrots",
    "slug": "parrots",
    "owner": "ahmetzeren",
    "version": "1.0.0",
    "scheme": "parrotsapp",
    "orientation": "portrait",
    "icon": "./assets/parrots-logo-new11.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/parrots-logo-new11.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.zenforest.parrots",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.938579686654-3l1dc47s6i61d0s2qif1cvajh3fnfkvq"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/parrots-logo-new11.jpeg",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.zenforest.parrots",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAsqIXNMISkZ0eprGc2iTLbiQk0QBtgq0c"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "@react-native-google-signin/google-signin",
      "expo-font",
      "expo-build-properties",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "5f4ee090-1e51-4228-b36b-23715333bcc4"
      }
    }
  }
}